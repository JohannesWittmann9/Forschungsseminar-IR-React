{
  "manifest_version": 3,
  "name": "My Chrome Extension",
  "version": "1.0",
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "background": {
    "service_worker": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["SERPs.js"]
    }
  ],
  "externally_connectable": {
    "matches": ["\\*://localhost/\\*"]
  }
}
